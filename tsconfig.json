{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "Node10",
    "esModuleInterop": true,
    "strict": true,
    "strictBindCallApply": false,
    "noImplicitAny": false,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "outDir": "./dist",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": false,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@test/*": ["test/*"],
      "@modules/*": ["src/modules/*"],
      "@apis/*": ["src/modules/apis/*"],
      "@database/*": ["src/modules/apis/database/*"],
      "@repositories/*": ["src/modules/repositories/*"],
      "@services/*": ["src/modules/services/*"],
      "@configs/*": ["src/common/configs/*"],
      "@utils/*": ["src/common/utils/*"],
      "@interceptors/*": ["src/common/interceptors/*"],
      "@filters/*": ["src/common/filters/*"],
      "@decorators/*": ["src/common/decorators/*"],
      "@middlewares/*": ["src/common/middlewares/*"],
      "@guards/*": ["src/common/guards/*"],
      "@exceptions/*": ["src/common/exceptions/*"],
      "@pipes/*": ["src/common/pipes/*"],
      "@models/*": ["src/models/*"],
      "@credentials/*": ["src/credentials/*"]
    }
  }
}
