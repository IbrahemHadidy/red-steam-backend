    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.13",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.0.13",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.1.1",
    "@nestjs/typeorm": "^11.0.0",
    "@nestlab/google-recaptcha": "^3.10.0",
    "@paypal/paypal-server-sdk": "^0.6.1",
    "ansi-regex": "^6.1.0",
    "cache-manager": "^6.4.2",
