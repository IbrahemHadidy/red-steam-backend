    "@fastify/redis": "^7.0.2",
    "@fastify/static": "^8.1.1",
    "@fastify/swagger": "^9.4.2",
    "@keyv/redis": "^4.3.3",
    "@nest-lab/fastify-multer": "^1.3.0",
