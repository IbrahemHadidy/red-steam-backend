{
  "root": true,
  "extends": [
    "eslint:recommended",
    "prettier",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "env": {
    "es2021": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "@typescript-eslint/no-unused-vars": "warn"
  }
}
